
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>Username</th>
        <th>Gender</th>
        <th>Roll</th>
    </tr>
    @foreach (var item in Model) { 
        <tr>
            <td>@item.Name</td>
            <td>@item.Username</td>
            <td>@item.Gender</td>
            <td>@item.Roll</td>
        </tr>
    }
</table>

